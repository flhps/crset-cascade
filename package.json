{
  "name": "crset-cascade",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Implementation of a padded bloom filter cascade for CRSet.",
  "type": "module",
  "keywords": [
    "crset bloom filter cascade"
  ],
  "homepage": "https://github.com/jfelixh/crset-cascade",
  "author": {
    "name": "Felix Hoops",
    "url": "https://github.com/jfelixh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jfelixh/crset-cascade.git"
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "dev": "rollup -c rollup.config.mjs -w",
    "clean": "rimraf dist",
    "test": "jest",
    "prepare": "npm run build",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "eslint": "^9.20.1",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.1",
    "rimraf": "^5.0.0",
    "rollup": "^4.31.0",
    "rollup-plugin-dts": "^6.1.1",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  }
}
